{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "pb-gallery/pb-image-row",
	"version": "1.0.1",
	"title": "Image Row",
	"category": "pb-gallery",
	"icon": "columns",
	"description": "Groups images into a single row, scaling them to fill the full width without cropping.",
	"example": {},
	"textdomain": "pb-gallery",
	"parent": [ "pb-gallery/modular-gallery-block" ],
	"allowedBlocks": [ 
		"pb-gallery/pb-image-block",
		"pb-gallery/pb-image-stack"
	],
	"supports": {
		"inserter": false,
		"anchor": false,
		"ariaLabel": true,
		"html": false
    },
	"providesContext": {
        "portfolioBlocks/stackImageheight": "stackImageheight"
	},
	"usesContext": [ 
		"portfolioBlocks/noGap",
		"portfolioBlocks/layoutVersion"
	],
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js"
}