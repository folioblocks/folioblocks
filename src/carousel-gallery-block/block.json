{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "folioblocks/carousel-gallery-block",
	"version": "1.0.3",
	"title": "Carousel Gallery",
	"category": "folioblocks",
	"allowedBlocks": [ "folioblocks/pb-image-block" ],
